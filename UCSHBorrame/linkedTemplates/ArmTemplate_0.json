						"     store: 'sqlserver',",
						"     schemaName: 'dbo',",
						"     tableName: 'QA_STG_ESTADO_POSTULANTE',",
						"     insertable: true,",
						"     updateable: false,",
						"     deletable: false,",
						"     upsertable: false,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          ESTADO_POSTULANTE_SK,",
						"          CODIGO_ESTADO_POSTULANTE,",
						"          ESTADO_POSTULANTE,",
						"          FECHA_CREACION,",
						"          FECHA_ACTUALIZACION = {FECHA_MODIFICACIÓN},",
						"          ORIGEN,",
						"          ETL_ID",
						"     )) ~> sink1"
